// dificilee.cpp : Defines the entry point for the console application.
//
#include "stdafx.h"
#include <iostream>
using namespace std;
#include <cmath>
int main()
{
	float a,b,c,d,e,f,x,y;
	cout<<"primera ecuacion";
	cin>>a>>b>>c;
	cout<<"segunda ecuacion";
	cin>>d>>e>>f;
	x=e*(c+e)/(e*a+d*b);
	cout<<"el valor de x es "<<x<<endl;
	y= (c-a*x)/b;
	cout<<"el valor de y es "<< y<<endl;
	system ("PAUSE");
	return 0;
}
