// bitch please.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
#include "conio.h"

using namespace std;

int _tmain(int argc, _TCHAR* argv[])
{
	int i,n,b;
	cout<<"ingrese n"<<endl;
	cin>>n;
	b=1;
	i=1;
	if (n<0)
	{
		cout<<"el numero no es valido ingrse otro"<<endl;
	    cin>>n;
	}
	else {
	while (n >= i)
	{ 

		b=b*i;
		i=i++;
	}
	}
	
	cout<<b;
	getch ();
	return 0;
}
